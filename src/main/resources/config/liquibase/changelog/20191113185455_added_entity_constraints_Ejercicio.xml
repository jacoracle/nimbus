<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Ejercicio.
    -->
    <changeSet id="20191113185455-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="componente_id"
                                 baseTableName="ejercicio"
                                 constraintName="fk_ejercicio_componente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="componente"/>

        <addForeignKeyConstraint baseColumnNames="ejercicio_id"
                                 baseTableName="ejercicio_tipoactividad"
                                 constraintName="fk_ejercicio_tipoactividad_ejercicio_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ejercicio"/>

        <addForeignKeyConstraint baseColumnNames="tipoactividad_id"
                                 baseTableName="ejercicio_tipoactividad"
                                 constraintName="fk_ejercicio_tipoactividad_tipoactividad_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tipo_actividad"/>
        
    </changeSet>
</databaseChangeLog>
